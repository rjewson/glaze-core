package glaze.geom;

class Rectangle {
    
    public var x:Float;
    public var y:Float;
    public var width:Float;
    public var height:Float;

    public function new(x = 0.,y = 0.,width = 0.,height = 0.) {
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
    }

}